{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "ignorePresets": [
    ":prHourlyLimit2"
  ],
  "docker": {
    "pinDigests": true
  },
  "regexManagers": [
    {
      "fileMatch": ["project.properties$"],
      "matchStrings": [
        "#\\s*renovate:\\s*(datasource=(?<datasource>.*?) )?depName=(?<depName>.*?)( versioning=(?<versioning>.*?))?\\s*.*?_version=(?<currentValue>.*)"
      ]
    }
  ]
}
